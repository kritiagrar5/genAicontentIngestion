{
  "_version": "1.76.0",
  "sap.app": {
    "id": "genaicontentingestion",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.18.6",
      "toolsId": "5c28b49d-c34b-4082-91d9-6a8189e6a548"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/catalog/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    },
      "crossNavigation": {
      "inbounds": {
        "genaicontentingestion": {
          "semanticObject": "genaicontentingestion",
          "action": "display",
          "title": "Content Ingestion",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.140.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "genaicontentingestion.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "ContentList",
          "target": "ContentList"
        }
      ],
      "targets": {
        "ContentList": {
          "type": "Component",
          "id": "ContentList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Content",
              "variantManagement": "Page",
              "navigation": {},
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  },
                  "columns": {
                    "FileDownload": {
                      "header": "File Name",
                      "position": {
                        "anchor": "DataField::mediaType",
                        "placement": "Before"
                      },
                      "template": "genaicontentingestion.ext.fragment.FileDownload",
                       "properties": ["fileName"]
                    },
                    "mediaType": {
                      "header": "Media Type",
                       "position": {
                        "anchor": "DataField::status",
                        "placement": "Before"
                      },
                      "template": "genaicontentingestion.ext.fragment.MediaType",
                        "properties": ["mediaType"]
                    },
                    "Metadata": {
                      "position": {
                        "anchor": "DataField::UseCase",
                        "placement": "After"
                      },
                      "template": "genaicontentingestion.ext.fragment.Metadata",
                      
                      "width": "5rem",
                      "horizontalAlign": "Center"
                    },
                    "Delete": {
                      "position": {
                        "anchor": "DataFieldForAction::CatalogService.rejectContent",
                        "placement": "After"
                      },
                      "template": "genaicontentingestion.ext.fragment.Delete",
                      "width": "5rem",
                      "horizontalAlign": "Center"
                    }
                  }
                }
              },
              "initialLoad": "Enabled",
              "liveMode": true,
              "content": {
                "header": {
                  "customHeader": {
                    "expandedHeaderFragment": "genaicontentingestion.fragment.UploadFile"
                  }
                }
              }
            }
          }
        }
      }
    },
    "extends": {
      "extensions": {
        "sap.ui.controllerExtensions": {
          "sap.fe.templates.ListReport.ListReportController": {
            "controllerName": "genaicontentingestion.controller.CustomExpandedHeader"
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.cloud": {
    "public": true,
    "service": "genaicontentingestion"
  }
}
